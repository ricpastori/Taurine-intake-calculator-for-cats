#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules/elm-format/bin/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules/elm-format/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules/elm-format/bin/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules/elm-format/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/elm-format@0.8.7/node_modules:/Users/riccardopastori/Documents/Progetti/Learn Elm/elm-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/elm-format" "$@"
else
  exec node  "$basedir/../../bin/elm-format" "$@"
fi
