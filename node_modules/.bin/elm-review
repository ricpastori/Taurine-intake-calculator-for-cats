#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules/elm-review/bin/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules/elm-review/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules/elm-review/bin/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules/elm-review/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/elm-review@2.12.0/node_modules:/Users/riccardopastori/Documents/Progetti/Learn_Elm/Taurine-intake-calculator-for-cats/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../elm-review/bin/elm-review" "$@"
else
  exec node  "$basedir/../elm-review/bin/elm-review" "$@"
fi
